<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bt.mapper.GoodsMapper">


    <insert id="addGoods" parameterType="com.bt.pojo.Goods">
        insert into s_goods
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="id != null">
                id,
            </if>
            <if test="title != null">
                `title`,
            </if>
            <if test="cateId != null">
                cate_id,
            </if>
            <if test="price != null">
                `price`,
            </if>
            <if test="prams != null">
                prams,
            </if>
            <if test="num != null">
                num,
            </if>
            <if test="desc != null">
                `desc`,
            </if>
            <if test="createTime != null">
                create_time,
            </if>
            <if test="updateTime != null">
                update_time,
            </if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="id != null">
                #{id},
            </if>
            <if test="title != null">
                #{title},
            </if>
            <if test="cateId != null">
                #{cateId},
            </if>
            <if test="price != null">
                #{price},
            </if>
            <if test="prams != null">
                #{prams},
            </if>
            <if test="num != null">
                #{num},
            </if>
            <if test="desc != null">
                #{desc},
            </if>
            <if test="createTime != null">
                #{createTime},
            </if>
            <if test="updateTime != null">
                #{updateTime},
            </if>
        </trim>
    </insert>
</mapper>
